        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Response status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "",
              "pm.test(\"Response has required fields - id, operationType, and amount\", function () {",
              "    const responseData = pm.response.json();",
              "    ",
              "    pm.expect(responseData).to.be.an('array');",
              "    responseData.forEach(item => {",
              "        pm.expect(item.id).to.be.a('string', \"id should be a string\");",
              "        pm.expect(item.operationType).to.exist.and.to.be.a('string', \"operationType should exist and be a string\");",
              "        pm.expect(item.amount).to.be.a('number', \"amount should be a number\");",
              "    });",
              "});",
              "",
              "",
              "pm.test(\"Amount is a non-negative integer\", function () {",
              "    const responseData = pm.response.json();",
              "    ",
              "    responseData.forEach(function(wallet) {",
              "        pm.expect(wallet.amount).to.be.a('number').and.to.be.at.least(0);",
              "    });",
              "});",
              "",
              "",
              "pm.test(\"Response time is less than 200ms\", function () {",
              "  pm.expect(pm.response.responseTime).to.be.below(200);",
              "});",
